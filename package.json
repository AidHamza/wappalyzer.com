{
  "name": "wappalyzer.com",
  "version": "1.0.0",
  "description": "wappalyzer.com",
  "author": "Wappalyzer",
  "scripts": {
    "dev": "ENVIRONMENT=beta nuxt",
    "prod": "NODE_ENV=production ENVIRONMENT=v2 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "deploy:beta": "ENVIRONMENT=beta nuxt build && aws s3 sync dist s3://beta.wappalyzer.com --cache-control max-age=31536000,public --exclude 'images/icons/*' --profile wappalyzer",
    "deploy:v2": "ENVIRONMENT=v2 nuxt build && aws s3 sync dist s3://www.wappalyzer.com --cache-control max-age=31536000,public --exclude 'images/icons/*' --profile wappalyzer"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.10.1",
    "@nuxtjs/dotenv": "^1.4.0",
    "amazon-cognito-identity-js": "^3.2.2",
    "nuxt": "^2.0.0",
    "nuxt-stripe-module": "^2.0.0",
    "nuxtjs-mdi-font": "^1.0.2",
    "prismjs": "^1.19.0",
    "stripe": "^8.46.0",
    "vue-prism-component": "^1.1.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/vuetify": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4"
  }
}
